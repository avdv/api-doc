# Routes
# This file defines all application routes (Higher priority routes first)



# apidoc

GET     /api/v1/api-docs                        no.samordnaopptak.apidoc.controllers.ApiDocController.get()
GET     /api/v1/api-docs/:path                  no.samordnaopptak.apidoc.controllers.ApiDocController.getPath(path: String)



# user

GET     /useradmin/api/v1/user                            no.samordnaopptak.apidoc.controllers.UserController.get
GET     /user2admin/api/v1/user                           no.samordnaopptak.apidoc.controllers.UserController.get2



# Map static resources

GET     /*file                          controllers.Assets.at(path="/public", file)
